{
  "name": "url-preview",
  "displayName": "URL Preview",
  "publisher": "your-name",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.80.0"
  },
  "activationEvents": [
    "onCommand:urlPreview.open"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "urlPreview.open",
        "title": "Open URL Preview"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./"
  },
  "devDependencies": {
    "@types/express": "^5.0.6",
    "@types/node": "^16.18.126",
    "@types/node-fetch": "^2.6.13",
    "@types/vscode": "^1.80.0",
    "typescript": "^4.9.0"
  },
  "dependencies": {
    "express": "^4.22.1",
    "node-fetch": "^2.7.0",
    "puppeteer-core": "^20.9.0"
  }
}
